{
   "kind":"Service",
   "apiVersion":"v1",
   "metadata":{
      "name":"indexlb",
      "annotations": {
         "service.beta.kubernetes.io/aws-load-balancer-type": "nlb"
      },
      "labels":{
         "app":"indexlb"
      }
   },
   "spec":{
      "ports": [
         {
           "port":80,
           "targetPort":80,
           "nodePort": 30516
         }
      ],
      "selector":{
         "app":"index"
      },
      "type": "LoadBalancer"
   }
}
